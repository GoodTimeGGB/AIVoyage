{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainAbility": "EntryAbility",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "ets/entryability/EntryAbility.ts",
        "type": "ui",
        "pages": "$profile:main_pages",
        "label": "$string:app_name"
      },
      {
        "name": "RouteServiceAbility",
        "srcEntry": "ets/service/RouteServiceAbility.ts",
        "type": "service",
        "label": "$string:service_name"
      }
    ],
    "requestPermissions": [
      { "name": "ohos.permission.INTERNET" },
      { "name": "ohos.permission.LOCATION" }
    ],
    "extensionAbilities": [
      {
        "name": "HomeFormExtension",
        "srcEntry": "ets/form/HomeFormExtension.ts",
        "type": "form"
      }
    ],
    "forms": [
      {
        "name": "home_card",
        "type": "JS",
        "uiSyntax": "ets",
        "description": "$string:home_card_desc",
        "jsComponentName": "HomeCard",
        "isDefault": true,
        "colorMode": "auto",
        "updateEnabled": false,
        "defaultDimension": "2*2",
        "supportDimensions": ["2*2", "2*4"]
      }
    ],
    "resources": {
      "base": {
        "profile": "src/main/resources/base/profile",
        "element": "src/main/resources/base/element"
      }
    }
  }
}
